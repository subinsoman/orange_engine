{
  "name": "deepsense.ai",
  "version": "1.4.4",
  "private": true,
  "description": "Seahorse",
  "author": "CodiLime, Inc.",
  "license": "ISC",
  "homepage": "https://deepsense.ai/",
  "repository": {
    "type": "git",
    "url": "https://github.com/deeepsense-ai/seahorse.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "cross-env NODE_ENV=production webpack -p --colors",
    "prelint": "eslint -c ./config/eslint/eslint-env.config.js ./*.js ./config",
    "lint": "eslint -c ./config/eslint/eslint-src.config.js ./client",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --progress --port 3000",
    "serve:dist": "cross-env NODE_ENV=production webpack-dev-server --progress --port 3000",
    "start": "npm run serve",
    "stop": "npm run clean",
    "pretest": "npm run lint",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "karma start --autoWatch=true --singleRun=false ./config/karma.conf.js"
  },
